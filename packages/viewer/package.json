  "main": "dist/node/index.js",
  "unpkg": "dist/umd/viewer.umd.min.js",
  "module": "dist/esm/index.js",
  "js:next": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "@esri/arcgis-rest-auth": "^2.4.0",
    "@esri/arcgis-rest-portal": "^2.4.0",
    "@esri/solution-common": "^0.5.1"
  },
  "dependencies": {
    "@esri/arcgis-rest-types": "^2.4.0",
    "tslib": "^1.10.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:node && npm run build:umd && npm run build:esm",
    "build:esm": "tsc -p ./tsconfig.json --module esnext --outDir ./dist/esm --declaration",
    "build:umd": "rollup -c ../../umd-base-profile.js && rollup -c ../../umd-production-profile.js",
    "build:node": "tsc -p ./tsconfig.json --module commonjs --outDir ./dist/node",
    "dev:esm": "tsc -w --module esnext --outDir ./dist/esm --declaration",
    "dev:umd": "rollup -w -c ../../umd-base-profile.js",
    "dev:node": "tsc -w --module commonjs --outDir ./dist/node"
  },
  "publishConfig": {
    "access": "public"
  },
