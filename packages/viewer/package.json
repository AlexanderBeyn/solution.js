{
  "name": "@esri/solution-viewer",
  "version": "0.0.1",
  "description": "to do",
  "main": "dist/node/index.js",
  "unpkg": "dist/umd/viewer.umd.js",
  "module": "dist/esm/index.js",
  "js:next": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "rollup": "^1.6.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:node && npm run build:umd && npm run build:esm",
    "build:esm": "tsc -p ./tsconfig.json --module es2015 --outDir ./dist/esm --declaration",
    "build:umd": "rollup -c ../../umd-base-profile.js && rollup -c ../../umd-production-profile.js",
    "build:node": "tsc -p ./tsconfig.json --module commonjs --outDir ./dist/node",
    "dev:esm": "tsc -w --module es2015 --outDir ./dist/esm --declaration",
    "dev:umd": "rollup -w -c ../../umd-base-profile.js",
    "dev:node": "tsc -w --module commonjs --outDir ./dist/node"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/solution.js.git"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/Esri/solution.js/issues"
  },
  "homepage": "https://github.com/Esri/solution.js#readme",
  "keywords": [
    "typescript",
    "promise",
    "fetch",
    "arcgis",
    "esri",
    "ES6"
  ]
}
